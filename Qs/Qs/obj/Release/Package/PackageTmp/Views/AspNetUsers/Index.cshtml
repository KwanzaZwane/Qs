@model IEnumerable<Qs.Models.AspNetUser>

@{
    ViewBag.Title = "Branch Management";
}

<h2>User Management</h2>

<br />

<a class="btn btn-success btn-circle btn-lg" href="@Url.Action("AddBranch","Account")">
    <i class="fa fa-user-plus" style="vertical-align:middle"></i>
</a>

<br />
<div class="dataTable_wrapper">
    <table class="table table-striped table-bordered table-hover display responsive" id="dataTables-example">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BranchGoogleId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BranchName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BranchLocation)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>


            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.BranchGoogleId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.BranchName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.BranchLocation)
                    </td>
                    <td>
                        <a href="@Url.Action("Index", "BranchFunctions", new { id=item.Id })" class="btn btn-info btn-circle">
                            <i class="fa fa-cogs "></i>
                        </a>
                        <a href="@Url.Action("Edit", "AspNetUsers", new { id=item.Id })" class="btn btn-warning btn-circle">
                            <i class="fa fa-pencil-square-o "></i>
                        </a>
                        <a href="@Url.Action("Details", "AspNetUsers", new { id = item.Id })" class="btn btn-primary btn-circle">
                            <i class="fa fa-list"></i>
                        </a>
                        <a href="@Url.Action("Delete", "AspNetUsers", new { id = item.Id })" class=" btn btn-danger btn-circle">
                            <i class="fa fa-trash-o"></i>
                        </a>

                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>


